<Page x:Class="WpfApp1.Stats"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApp1"
      mc:Ignorable="d" 
      d:DesignHeight="812" d:DesignWidth="375"
      Title="Stats">

    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/b.png"/>
        </Grid.Background>
        <Button Content="Button" HorizontalAlignment="Left" Height="26" Margin="35,771,0,0" VerticalAlignment="Top" Width="28" Click="Button_Click" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}"/>
        <Button x:Name="StatsB" Content="Button" HorizontalAlignment="Center" Height="60" Margin="0,735,0,0" VerticalAlignment="Top" Width="60" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}" Click="StatsB_Click"/>
        <Button x:Name="popol" Content="Button" HorizontalAlignment="Left" Height="42" Margin="25,253,0,0" VerticalAlignment="Top" Width="333" Click="popol_Click" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}"/>

    </Grid>
</Page>
