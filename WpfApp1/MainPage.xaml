            <Button Content="Button" Height="58" Canvas.Left="155" Canvas.Top="734" Width="60" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}" Click="Button_Click" HorizontalAlignment="Center" VerticalAlignment="Top"/>
            <Button x:Name="prB" Content="Button" Height="22" Canvas.Left="39" Canvas.Top="771" Width="23" Click="prB_Click" BorderBrush="{x:Null}" Foreground="{x:Null}" Background="{x:Null}"/>
            <Button x:Name="Proff" Content="Button" Height="41" Canvas.Left="26" Canvas.Top="54" Width="41" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}" HorizontalAlignment="Left" VerticalAlignment="Center" Click="Proff_Click"/>
        </Canvas>

    </Border>

</Page>
